{
  "name": "test-ssr-react",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "node server.mjs",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.jsx --outDir dist/server",
    "generate": "vite build --outDir dist/static && npm run build:server && node prerender.mjs",
    "serve": "cross-env NODE_ENV=production node server.mjs",
    "debug": "node --inspect-brk server.mjs"
  },
  "type": "module",
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "file:.yalc/react-router-dom"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "1.0.1",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "express": "4.17.1",
    "serve-static": "1.14.1",
    "vite": "2.5.10"
  }
}
